{
    "spec_dir": "spec",
    "spec_files": ["../**/tests/*[s]pec.js",  "spec/**/*[sS]pec.js"],
    "helpers": ["../**/tests/helpers/*.js"],
    "reporters": [
      {
        "name": "jasmine-spec-reporter#SpecReporter",
        "options": {
          "displayStacktrace": "all",
          "displaySuccessful": true
        }
      }
    ],
    "jsLoader": "require",
    "stopSpecOnExpectationFailure": false,
    "random": false
  }